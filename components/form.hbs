<section id="forms-block" class="forms">
	{{#with forms}}
	<div class="forms__container wrapper">
		<h1 class="forms__title --rubrica">{{title}}</h1>
		<h3 class="forms__subtitle">{{{breaklines subtitle}}}</h3>

		<form  class="forms__inputs">
			<div class="inputs">
				<input required id="user-name" class="inputs__input form-input" placeholder="имя" type="text">
				<input required id="user-email" class="inputs__input form-input" placeholder="E-mail" type="email">
				<input required id="phone" class="inputs__input form-input" placeholder="номер телефона" type="tel">
				<input required id="company-name" class="inputs__input form-input" placeholder="название компании" type="text">
			</div>

			<div class="forms__checkboxes">

				<div class="checkboxes__title forms__subtitle">отметьте необходимое</div>

				<div class="checkboxes__blocks">
					{{#each items}}
					<div class="checkboxes__block">
						<div class="block__img --{{@key}}"></div>
						<div class="block__title --rubrica">{{name}}</div>
						<ul class="block__list">
							{{#each subitems}}
							<li class="block-list__item md-checkbox">
								<label class="pure-material-checkbox">
									<input class="checkbox-item" data-price="{{price}}" type="checkbox">
									<span class="checkbox-label"> {{name}}</span>
								</label>
							</li>
							{{/each}}
						</ul>
					</div>
					{{/each}}
				</div>


			</div>

